    <!-- Ranger Rick -->

    <FCP.Core.CharacterDef MayRequire="Rick.FCP.NCR">
        <defName>FCP_Character_Ranger_Rick</defName>
        <pawnKind>FCP_Unique_Ranger_Rick</pawnKind>
        <xenotype>Baseliner</xenotype>
        <faction MayRequire="Rick.FCP.NCR">FCP_Faction_NCR</faction>
        <roles>
        </roles>
        <definitions>
            <!-- Bio -->
            <li Class="FCP.Core.CharacterStoryDefinition">
                <firstName>Rick</firstName>
                <lastName>Farnsworth</lastName>
                <nickname>Ranger Rick</nickname>
                <gender>Male</gender>
                <age>27</age>
            </li>
            <!-- Appearance -->
            <li Class="FCP.Core.CharacterAppearanceDefinition">
                <hairColor>(88, 60, 51)</hairColor>
                <hairDef>FCP_Hair_Ranger_Rick</hairDef>
                <beardDef>NoBeard</beardDef>
                <skinColorOverride>(239, 189, 163)</skinColorOverride>
                <headTypeDef>Male_AverageNormal</headTypeDef>
                <bodyTypeDef>Male</bodyTypeDef>
            </li>
            <!-- Factional -->
            <li Class="FCP.Core.CharacterTitleDefinition" MayRequire="Rick.FCP.NCR">
                <title>FCP_Title_NCR_Ranger_Volunteer</title>
            </li>
            <!-- Unique Apparels -->
            <li Class="FCP.Core.CharacterUniqueItemDefinition" MayRequire="Rick.FCP.NCR">
                <uniqueItem>FCP_Apparel_Ranger_Ricks_Uniform</uniqueItem>
            </li>
            <li Class="FCP.Core.CharacterUniqueItemDefinition" MayRequire="Rick.FCP.NCR">
                <uniqueItem>FCP_Apparel_Ranger_Ricks_Armor</uniqueItem>
            </li>
            <li Class="FCP.Core.CharacterUniqueItemDefinition" MayRequire="Rick.FCP.NCR">
                <uniqueItem>FCP_Apparel_Ranger_Ricks_Duster</uniqueItem>
            </li>
            <li Class="FCP.Core.CharacterUniqueItemDefinition" MayRequire="Rick.FCP.NCR">
                <uniqueItem>FCP_Apparel_Ranger_Ricks_Helmet</uniqueItem>
            </li>
        </definitions>
    </FCP.Core.CharacterDef>

