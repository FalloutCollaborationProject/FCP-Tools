        <defName>FCP_EquipFromStation</defName>
        <driverClass>RangerRick_PowerArmor.JobDriver_EquipFromStation</driverClass>
        <reportString>equipping power armor from TargetA.</reportString>
    </JobDef>

    <JobDef>
        <defName>FCP_SwapPowerArmor</defName>
        <driverClass>RangerRick_PowerArmor.JobDriver_SwapPowerArmor</driverClass>
        <reportString>swapping power armor at TargetA.</reportString>
    </JobDef>
    
    <JobDef>
        <defName>RR_RepairPowerArmor</defName>
        <driverClass>RangerRick_PowerArmor.JobDriver_RepairPowerArmor</driverClass>
