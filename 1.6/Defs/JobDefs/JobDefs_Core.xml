<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <JobDef>
        <defName>RR_RefuelPowerArmor</defName>
        <driverClass>RangerRick_PowerArmor.JobDriver_RefuelPowerArmor</driverClass>
        <reportString>Refueling TargetA.</reportString>
        <allowOpportunisticPrefix>true</allowOpportunisticPrefix>
    </JobDef>

    <JobDef>
        <defName>RR_TrainTrait</defName>
        <driverClass>RangerRick_PowerArmor.JobDriver_TrainTrait</driverClass>
