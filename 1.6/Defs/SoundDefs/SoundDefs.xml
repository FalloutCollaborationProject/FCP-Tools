    <!--_____ Lunchbox _____-->

    <!-- Interact -->

    <SoundDef>
        <defName>FCP_Sound_Interact_Lunchbox</defName>
        <context>MapOnly</context>
        <maxSimultaneous>1</maxSimultaneous>
        <subSounds>
            <li>
                <grains>
