    <!-- 20 Gauge Shell -->

    <ThingDef ParentName="BaseBullet">
        <defName>FCP_Bullet_20Gauge_Shell</defName>
        <label>20 Gauge Shell</label>
        <graphicData>
            <texPath>Things/Misc/Bullets/FCP_20G_Pellet</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>TransparentPostLight</shaderType>
        </graphicData>
        <thingClass>FragProjectile.Projectile_FragExplosion</thingClass>
        <projectile>
            <speed>70</speed>
            <damageDef>Bullet</damageDef>
            <damageAmountBase>2</damageAmountBase>
            <explosionRadius>0</explosionRadius>
            <armorPenetrationBase>0</armorPenetrationBase>
            <stoppingPower>1</stoppingPower>
            <ai_IsIncendiary>false</ai_IsIncendiary>
        </projectile>
        <modExtensions>
            <li Class="FragProjectile.ProjectileExtension_Fragmentation">
                <projCount>7</projCount>
                <radius>3~5</radius>
                <isCone>true</isCone>
                <projectileDef>FCP_Bullet_20Gauge_Pellet</projectileDef>
            </li>
        </modExtensions>
    </ThingDef>

