            <stoppingPower>1</stoppingPower>
            <ai_IsIncendiary>false</ai_IsIncendiary>
        </projectile>
        <modExtensions>
            <li Class="FragProjectile.ProjectileExtension_Fragmentation">
                <projCount>7</projCount>                <!-- Amount of frag projectile that can spawn -->
                <radius>3~5</radius>                <!-- How far the frag projectile can travel before reaching ground -->
                <isCone>true</isCone>                <!-- An indicator where the frag projectile can launch in a cone-like trajectory instead of radial trajectory -->
                <projectileDef>FCP_Bullet_12Gauge_Pellet</projectileDef>                <!-- Type of bullet for the frag projectile -->
            </li>
        </modExtensions>
    </ThingDef>

